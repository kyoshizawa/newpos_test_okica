<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_head_navigation"
    app:startDestination="@+id/navigation_menu_head">

    <fragment
        android:id="@+id/navigation_menu_head"
        android:name="jp.mcapps.android.multi_payment_terminal.ui.menu.MenuHeadFragment"
        tools:layout="@layout/fragment_menu_head" >
        <action
            android:id="@+id/action_navigation_menu_head_to_navigation_menu_head_empty"
            app:destination="@id/navigation_menu_head_empty"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_right"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"
            app:popUpTo="@id/navigation_menu_head_empty"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/navigation_menu_head_empty"
        android:name="jp.mcapps.android.multi_payment_terminal.ui.menu.MenuHeadEmptyFragment"
        tools:layout="@layout/fragment_menu_head_empty" >
        <action
            android:id="@+id/action_navigation_menu_head_empty_to_navigation_menu_head"
            app:destination="@id/navigation_menu_head"
            app:popUpTo="@id/navigation_menu_head"
            app:popUpToInclusive="true" />
    </fragment>

</navigation>